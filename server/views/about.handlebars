<script>
function init_panels(){
	if (!$("ul.panels").length){
		return;
	}
	var speed=200;
	$("ul.panels li:last-child").addClass("last");
	
	$("ul.panels li").hover(function(){
		$(this).stop().animate({
			width:"360px",
			fontSize:"150px"
		},speed)
		.siblings("li").stop().animate({
			width:"135px",
			fontSize:"50px"
		},speed);
	},
	function(){
		$(this).stop().animate({
			width:"180px",
			fontSize:"100px"
		},speed)
		.siblings("li").stop().animate({
			width:"180px",
			fontSize:"100px"
		},speed);
	}
	);
}
init_panels();
	$("#mainMenu").menu();
